<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <com.virgo.financeloan.ui.view.UploadPicView
        android:id="@+id/maintain_pic_img"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:scaleType="centerCrop"
        tools:src="@mipmap/icon_add_pic"/>

    <ImageView
        android:id="@+id/maintain_pic_clear_img"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/delet_pic"
        android:layout_alignRight="@id/maintain_pic_img"
        android:layout_alignTop="@id/maintain_pic_img"/>

</RelativeLayout>
